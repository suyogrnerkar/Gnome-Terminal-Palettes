[global_config]
  handle_size = 0
  title_hide_sizetext = True
  title_transmit_fg_color = "#dedede"
  title_inactive_fg_color = "#ffffff"
  enabled_plugins = InactivityWatch, ActivityWatch, TerminalShot, LaunchpadCodeURLHandler, APTURLHandler, Logger, LaunchpadBugURLHandler
  title_transmit_bg_color = "#4c44a3"
  inactive_color_offset = 1.0
  title_inactive_bg_color = "#006651"
[keybindings]
  reset = <Primary><Shift>o
  split_horiz = <Primary><Shift>e
  split_vert = <Primary><Shift>r
[profiles]
  [[default]]
    palette = "#000000:#ff4321:#75c400:#ffbb28:#00ce98:#aa00aa:#00aaaa:#aaaaaa:#555555:#ff2d47:#55ff55:#ffff55:#5b5bed:#009f83:#1b98ba:#7d7d7d"
    copy_on_selection = True
    background_image = None
    background_darkness = 0.93
    background_type = transparent
    use_system_font = False
    cursor_color = "#ff3c0a"
    foreground_color = "#c2c2c2"
    use_custom_command = True
    login_shell = True
    custom_command = /bin/zsh --login
    cursor_shape = underline
    font = Droid Sans Mono 11
    background_color = "#121212"
    scrollback_infinite = True
  [[OnePlus]]
    palette = "#000000:#cc0000:#4e9a06:#c4a000:#3465a4:#75507b:#06989a:#d3d7cf:#555753:#ef2929:#8ae234:#fce94f:#729fcf:#ad7fa8:#34e2e2:#eeeeec"
    login_shell = True
    background_darkness = 0.9
    background_image = None
    cursor_color = "#00cdf9"
    foreground_color = "#ffffff"
    use_custom_command = True
    custom_command = /bin/zsh --login
    copy_on_selection = True
    scrollback_infinite = True
[layouts]
  [[default]]
    [[[child1]]]
      profile = New Profile
      type = Terminal
      parent = window0
    [[[window0]]]
      type = Window
      parent = ""
  [[New Layout]]
    [[[child0]]]
      position = 1600:0
      type = Window
      order = 0
      parent = ""
      size = 1600, 873
    [[[terminal1]]]
      profile = default
      type = Terminal
      order = 0
      parent = child0
[plugins]
